{
  "name": "codestar-phoenix",
  "license": "MIT",
  "scripts": {
    "build": "lerna run build --stream",
    "release": "lerna publish",
    "uninstall-packages": "lerna run uninstall-packages --stream",
    "install-packages": "lerna run install-packages --stream",
    "reinstall-packages": "lerna run reinstall-packages --stream",
    "uninstall-material-ui": "lerna run uninstall-material-ui --stream",
    "install-material-ui": "lerna run install-material-ui --stream",
    "reinstall-material-ui": "lerna run reinstall-material-ui --stream",
    "start-storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .storybook-static",
    "deploy-storybook": "gh-pages -d .storybook-static",
    "test": "lerna exec --concurrency 1 -- npm run test",
    "lint": "lerna run --stream lint",
    "lint-fix": "lerna run --parallel stream-fix",
    "ci": "lerna run ci --stream",
    "cd": "npm run build-storybook",
    "ci-cd": "npm-run-all ci cd"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "pre-commit": [
    "lint:staged"
  ],
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --syntax=scss --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "@babel/plugin-proposal-class-properties": "7.2.1",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-react": "7.0.0",
    "@storybook/addon-info": "^4.0.11",
    "@storybook/addon-notes": "^4.0.11",
    "@storybook/addon-options": "^4.0.11",
    "@storybook/addons": "^4.0.11",
    "@storybook/react": "^4.0.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "1.0.0",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "gh-pages": "^2.0.1",
    "git-revision-webpack-plugin": "3.0.3",
    "husky": "1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "23.6.0",
    "jest-enzyme": "7.0.1",
    "lerna": "^3.5.1",
    "lint-staged": "8.1.0",
    "nodemon": "1.18.7",
    "npm-run-all": "4.1.5",
    "prettier": "1.15.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-hot-loader": "4.3.12",
    "stylelint": "9.9.0",
    "stylelint-order": "2.0.0",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.27.0",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "4.1.4"
  }
}
